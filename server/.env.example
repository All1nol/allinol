PORT=5000
MONGO_URI=mongodb://localhost:27017/allinol
NODE_ENV=development
# Add your OpenAI API key for AI-powered PRD analysis
OPENAI_API_KEY=your_openai_api_key_here
# Add your Groq API key for LLM service
GROQ_API_KEY=your_groq_api_key_here
# Configure Groq model (optional, defaults to llama-3.1-70b-versatile)
GROQ_MODEL=llama-3.1-70b-versatile
# Configure Groq rate limit (optional, defaults to 60 requests per minute)
GROQ_RATE_LIMIT=60 